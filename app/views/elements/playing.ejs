<div class="container-fluid">
      <legend class="text-center"> Now Playing </legend>
      <div class="well">
        <div class="row">
          <div class="playing">
              <% if(queue && queue[0]) { %>
              <% for(var i=0; i < 1; i++) { %>
              <div class="col-md-6 col-md-offset-3 text-center">
                <a type="button" class="btn btn-lg btn-primary completeGame" id="<%= queue[i]._id %>" value="<%= queue[i].player1 %>">
                  <%= queue[i].player1 %>&nbsp;
                  <i class="fa fa-trophy fa-lg fa-fw text-right"></i>
                </a>
                <label><h4> vs. </h4></label>
                <a type="button" class="btn btn-lg btn-primary completeGame" id="<%= queue[i]._id %>" value="<%= queue[i].player2 %>" >
                  <i class="fa fa-trophy fa-lg fa-fw text-left"></i>&nbsp;
                  <%= queue[i].player2 %>
                </a>
              </div>
              <div class="col-md-1 col-md-offset-2">
                <a class="btn btn-lg btn-danger deleteGame" role="button" id="<%= queue[i]._id %>" >
                  <i class="fa fa-close fa-fw" aria-hidden="true"></i>
                </a>
              </div>
              <% } %>
              <% } else { %>
                <div class="row text-center">
                  <h3> Naebody&nbsp;vs.&nbsp;Naebody<h3>
                </div>
                <div class="row text-center">
                  <p> Why not queue? </p>
                </div>
             <% } %>
          </div>
        </div>
      </div>
</div>
